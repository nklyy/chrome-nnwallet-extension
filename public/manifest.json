{
  "name": "NoName Wallet",
  "description": "NoName Chrome Extension Wallet",
  "author": "NoName",
  "version": "0.0.1",
  "icons": {
    "16": "./icons/16.ico",
    "32": "./icons/32.ico",
    "48": "./icons/48.ico",
    "128": "./icons/128.ico"
  },
  "permissions": [
    "storage",
    "activeTab"
  ],
  "browser_action": {
    "default_title": "NoName Chrome Extension Wallet",
    "default_popup": "./index.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}

